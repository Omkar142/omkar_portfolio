  <script>
    // Smooth scrolling for anchor links
    document.querySelectorAll('a[href^="#"]').forEach(anchor => {
      anchor.addEventListener('click', function(e) {
        e.preventDefault();
        const targetId = this.getAttribute('href');
        const targetElement = document.querySelector(targetId);

        if (targetElement) {
             targetElement.scrollIntoView({
                behavior: 'smooth'
            });
        } else {
            console.warn(`Smooth scroll target not found: ${targetId}`);
        }
      });
    });

    // Sticky navbar on scroll
    window.addEventListener('scroll', function() {
      const nav = document.querySelector('nav');
      if (window.scrollY > 50) {
        nav.style.padding = '0.8rem 0';
        nav.style.boxShadow = '0 5px 15px rgba(0, 0, 0, 0.1)';
      } else {
        nav.style.padding = '1.5rem 0';
        nav.style.boxShadow = 'none';
      }
    });

     // Basic form submission prevention for demo
    const contactForm = document.querySelector('.contact-form form');
    if (contactForm) {
        contactForm.addEventListener('submit', function(e) {
            e.preventDefault();
            alert('Contact form submitted (Demo)! Implement backend connection.');
            this.reset(); // Optional: clear form after fake submit
        });
    }

    // Update copyright year dynamically
    const copyrightYear = document.querySelector('.copyright');
    if(copyrightYear) {
        copyrightYear.textContent = `© ${new Date().getFullYear()} Omkar Gochhayat. All rights reserved.`;
    }

  </script>
